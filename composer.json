{
    "name": "domainvalidity/domainvalidity",
    "description": "Light PHP SDK to interact with the Doma(in)Validity API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adro Rocker",
            "email": "me@adro.rocks"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "psr/http-client": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.6",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Domainvalidity\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Domainvalidity\\Sdk\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
